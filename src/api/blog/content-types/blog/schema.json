{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "blog_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-category.blog-category",
      "mappedBy": "blogs"
    },
    "blog_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-tag.blog-tag",
      "mappedBy": "blogs"
    },
    "date_created": {
      "type": "date"
    },
    "new_content": {
      "type": "blocks"
    },
    "old_content": {
      "type": "richtext"
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "description": {
      "type": "text"
    },
    "blog_id": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
